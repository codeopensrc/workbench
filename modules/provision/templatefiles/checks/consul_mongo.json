{
  "service": {
    "name": "mongo",
    "tags": ["_port=${port}", "_version=${version}"],
    "address": "",
    "port": "${port}",
    "enable_tag_override": false,
    "checks": [
      {
        "args": ["mongo", "--host", "${ip_address}", "--eval", "'exit'"],
        "interval": "30s"
      }
    ]
  }
}
