{
  "service": {
    "name": "pg",
    "tags": ["_port=${port}", "_version=${version}"],
    "address": "",
    "port": "${port}",
    "enable_tag_override": false,
    "checks": [
      {
        "args": ["echo", "'\\q'", "|", "psql", "postgresql://pgcheck:${read_only_pw}@${ip_address}/postgres"],
        "interval": "30s"
      }
    ]
  }
}
